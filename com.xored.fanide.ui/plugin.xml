          name="%FanTodoPropertyPage.name">
      <enabledWhen>
        <adapt type="org.eclipse.core.resources.IProject">
          <test property="org.eclipse.core.resources.projectNature" value="com.xored.fanide.core.nature"/>
        </adapt>         
