            %NewEnumWizard.description
         </description>
      </wizard>
      <wizard
            category="com.xored.fanide.ui"
            class="com.xored.fanide.internal.ui.wizards.FanNewScriptWizard"
            icon="icons/etool16/newfile_wiz.gif"
            id="com.xored.fanide.ui.internal.wizards.newscript"
            name="%wizard.name.3">
