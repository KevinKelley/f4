------=_.content-0a7243a0-75d3-3d5f-9791-539de0e5b7ac
Content-Type: text/ecl
Entry-Name: .content

get-view "Fantom Explorer" | get-tree | select "fantest/inheritance" | get-menu "New/Fantom Class" | click
with [get-window "New Fantom Class"] {
    get-editbox -after [get-label "Name:"] | set-text ClassWithSuperClass
    get-button "Browse..." -after [get-label "Superclass:"] | click
    with [get-window "Extended Classes Selection"] {
        get-editbox -after [get-label "Choose superclass:"] | set-text c
        get-table | select CancelledErr
    }
}
get-window "New Fantom Class" | get-window "Extended Classes Selection" | get-table | get-property itemCount | equals 8 
    | verify-true
get-window "New Fantom Class" | get-window "Extended Classes Selection" | get-table | select ClassB
get-window "New Fantom Class" | get-window "Extended Classes Selection" | get-label "ClassB - fantest/inheritance" 
    | get-property caption | equals "ClassB - fantest/inheritance" | verify-true
get-window "New Fantom Class" | get-window "Extended Classes Selection" | get-button OK | click
get-window "New Fantom Class" | get-editbox -after [get-label "Superclass:"] | get-property value | equals ClassB 
    | verify-true
get-window "New Fantom Class" | get-button Finish | click
get-editor "ClassWithSuperClass.fan" | get-editbox | get-property value | equals "class ClassWithSuperClass : ClassB{}" 
    | verify-true
get-view "Fantom Explorer" | get-tree | select "fantest/fan" | get-menu "New/Fantom Class" | click
with [get-window "New Fantom Class"] {
    get-editbox -after [get-label "Name:"] | set-text ClassWithSuperClass2
    get-editbox -after [get-label "Superclass:"] | set-text ClassA
    get-button Finish | click
}
get-editor "ClassWithSuperClass2.fan" | get-editbox | get-property value 
    | equals "class ClassWithSuperClass2 : ClassA{}" | verify-true
------=_.content-0a7243a0-75d3-3d5f-9791-539de0e5b7ac--
