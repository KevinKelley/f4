  <content xsi:type="org.eclipse.ecl.core:Script">
    <content><![CDATA[get-view "Fantom Explorer" | get-tree | select TestProject | get-menu "New/Empty Fantom Script" | click
get-window "New Fantom Script" | get-editbox -after [get-label "Source folder:"] | get-property value 
    | equals "TestProject/fan" | verify-true
with [get-window "New Fantom Script"] {
    get-editbox -after [get-label "Name:"] | set-text testScript
    get-button Finish | click
}
get-tab-folder | get-property pages -index 0 | equals "testScript.fan" | verify-true
get-editor "testScript.fan" | get-editbox | get-property value | contains "// Copyright " | verify-true
with [get-view "Fantom Explorer" | get-tree] {
    get-item TestProject | get-property childCount | equals 5 | verify-true
    get-item "TestProject/fan" | get-property caption | equals fan | verify-true
    get-item "TestProject/fan" | get-property childCount | equals 2 | verify-true
    get-item "TestProject/fan/A.fan" | get-property caption | equals "A.fan" | verify-true
    get-item "TestProject/fan/testScript.fan" | get-property caption | equals "testScript.fan" | verify-true
    get-item "TestProject/fan/testScript.fan" | get-property childCount | equals 0 | verify-true
}
get-view Outline | get-tree | get-property itemCount | equals 0 | verify-true]]></content>
  </content>
