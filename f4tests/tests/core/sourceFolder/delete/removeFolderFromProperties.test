  <content xsi:type="org.eclipse.ecl.core:Script">
    <content><![CDATA[get-view "Fantom Explorer" | get-tree | select fantest | get-menu Properties | click
with [get-window "Properties for fantest"] {
    get-tree | select "Fantom/Build Path"
    get-tab-folder | get-tab-item Source | click
    get-tree -after [get-label "Source folders on build path:"] | select "fantest\\/inheritance"
}
with [get-window "Properties for fantest"] {
    get-button "Edit..." | get-property enablement | equals true | verify-true
    get-button Remove | get-property enablement | equals true | verify-true
}
get-window "Properties for fantest" | get-button Remove | click
with [get-window "Properties for fantest"] {
    get-button "Edit..." | get-property enablement | equals false | verify-true
    get-button Remove | get-property enablement | equals false | verify-true
}
get-window "Properties for fantest" | get-button OK | click
get-view "Fantom Explorer" | get-tree | get-item fantest | get-property childCount | equals 6 | verify-true
with [get-view "Fantom Explorer" | get-tree] {
    get-item "fantest/fan" | get-property caption | equals fan | verify-true
    get-item "fantest/fan" | get-property childCount | equals 4 | verify-true
    get-item "fantest/inheritance" | get-property caption | equals inheritance | verify-true
    get-item "fantest/inheritance" | get-property childCount | equals 6 | verify-true
}
get-view "Fantom Explorer" | get-tree | select "fantest/build.fan" | double-click -nowait
get-editor "build.fan" | get-editbox | get-property value 
    | equals "using buildclass Build : build::BuildPod{  new make()  {    podName = \"fantest\"    summary = \"\"    srcDirs = [`fan/`]    depends = [\"sys 1.0\"]  }}" 
    | verify-true
get-view "Fantom Explorer" | get-tree | select fantest | get-menu Properties | click
with [get-window "Properties for fantest"] {
    get-tree -after [get-label "Source folders on build path:"] | select "fantest\\/fan"
    get-button Remove | click
}
get-window "Properties for fantest" | get-tree -after [get-label "Source folders on build path:"] 
    | get-property itemCount | equals 0 | verify-true
get-window "Properties for fantest" | get-button Cancel | click
with [get-view "Fantom Explorer" | get-tree] {
    get-item "fantest/fan" | get-property caption | equals fan | verify-true
    get-item "fantest/fan" | get-property childCount | equals 4 | verify-true
}
get-view "Fantom Explorer" | get-tree | select "fantest/build.fan" | double-click -nowait
get-editor "build.fan" | get-editbox | get-property value 
    | equals "using buildclass Build : build::BuildPod{  new make()  {    podName = \"fantest\"    summary = \"\"    srcDirs = [`fan/`]    depends = [\"sys 1.0\"]  }}" 
    | verify-true]]></content>
  </content>
