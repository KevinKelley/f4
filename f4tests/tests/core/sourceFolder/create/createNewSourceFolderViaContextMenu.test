<com.xored.q7.scenario:Scenario xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:com.xored.q7.scenario="http://com/xored/q7/scenario.ecore" xmlns:com.xored.tesla.core.protocol="http:///com/xored/tesla/core/protocol.ecore" xmlns:com.xored.tesla.core.protocol.raw="http:///com/xored/tesla/core/protocol/raw.ecore" xmlns:com.xored.tesla.core.ui="http:///com/xored/tesla/core/ui.ecore" xmlns:ecore="http://www.eclipse.org/emf/2002/Ecore" xmlns:org.eclipse.ecl.core="http://www.eclipse.org/ecl/core.ecore" name="createNewSourceFolderViaContextMenu" version="3.0" id="_Ha0p8N3iEeC824MXpzkxKA" description="1. Select Fantom Project&#xD;&#xA;2. Select &quot;Build Path/New Folder Source...&quot; in context menu&#xD;&#xA;3. Set folder name and press finish&#xD;&#xA;4. Make sure that this folder is added in build.fan and in properties " tags="core/sourcefolder/create" type="ecl">
  <contexts>_g4UEsMyOEeCqoItsrYQC5Q</contexts>
  <contexts>_ex81IMyOEeCqoItsrYQC5Q</contexts>
