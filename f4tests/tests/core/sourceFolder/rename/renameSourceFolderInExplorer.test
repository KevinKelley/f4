<com.xored.q7.scenario:Scenario xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:com.xored.q7.scenario="http://com/xored/q7/scenario.ecore" xmlns:com.xored.tesla.core.protocol="http:///com/xored/tesla/core/protocol.ecore" xmlns:com.xored.tesla.core.protocol.raw="http:///com/xored/tesla/core/protocol/raw.ecore" xmlns:com.xored.tesla.core.ui="http:///com/xored/tesla/core/ui.ecore" xmlns:ecore="http://www.eclipse.org/emf/2002/Ecore" xmlns:org.eclipse.ecl.core="http://www.eclipse.org/ecl/core.ecore" name="renameSourceFolderInExplorer" version="3.0" id="_wINI8d3qEeCb36lRJMHQOw" description="1. Select source folder from project in Fantom Explorer&#xD;&#xA;2. Rename this folder&#xD;&#xA;3. Make sure that folder is renamed&#xD;&#xA;4. Make sure that in build.fan file new folder is displayed&#xD;&#xA;5. Make sure that in Properties of project new folder name is displayed" tags="core/sourcefolder/rename" type="ecl">
  <contexts>_g4UEsMyOEeCqoItsrYQC5Q</contexts>
  <contexts>_ex81IMyOEeCqoItsrYQC5Q</contexts>
  <contexts>_2spO4NkfEeCWXvk8UUBU4Q</contexts>
  <content xsi:type="org.eclipse.ecl.core:Script">
    <content><![CDATA[get-view "Fantom Explorer" | get-tree | select "fantest/inheritance" | get-menu "Refactor/Rename..." | click
get-window "Rename Source Folder" | get-editbox -after [get-label "New name:"] | set-text testFolder
get-window "Rename Source Folder" | get-button OK | click
with [get-view "Fantom Explorer" | get-tree] {
    get-item fantest | get-property caption | equals fantest | verify-true
    get-item fantest | get-property childCount | equals 6 | verify-true
    get-item "fantest/fan" | get-property caption | equals fan | verify-true
    get-item "fantest/testFolder" | get-property caption | equals testFolder | verify-true
}
get-view "Fantom Explorer" | get-tree | select "fantest/build.fan" | double-click -nowait
get-editor "build.fan" | get-editbox | get-property value 
    | equals "using buildclass Build : build::BuildPod{  new make()  {    podName = \"fantest\"    summary = \"\"    srcDirs = [`testFolder/`, `fan/`]    depends = [\"sys 1.0\"]  }}" 
    | verify-true
get-view "Fantom Explorer" | get-tree | select fantest | get-menu Properties | click
with [get-window "Properties for fantest"] {
    get-tree | select "Fantom/Build Path"
    get-tab-folder | get-tab-item Source | click
}
with [get-window "Properties for fantest" | get-tree -after [get-label "Source folders on build path:"]] {
    get-property itemCount | equals 2 | verify-true
    get-item "fantest\\/fan" | get-property caption | equals "fantest/fan" | verify-true
    get-item "fantest\\/testFolder" | get-property caption | equals "fantest/testFolder" | verify-true
}
get-window "Properties for fantest" | get-button Cancel | click]]></content>
  </content>
