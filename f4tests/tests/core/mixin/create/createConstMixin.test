------=_.content-0a7243a0-75d3-3d5f-9791-539de0e5b7ac
Entry-Name: .content
Content-Type: text/ecl

get-view "Fantom Explorer" | get-tree | select "fantest/fan" | get-menu "New/Fantom Mixin" | click
with [get-window "New Fantom Mixin"] {
    get-editbox -after [get-label "Name:"] | set-text MixinTest
    get-button const | click
    get-button Finish | click
}
get-editor "MixinTest.fan" | get-editbox | get-property value | equals "const mixin MixinTest{}" | verify-true
