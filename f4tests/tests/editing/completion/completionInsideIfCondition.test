  <content xsi:type="org.eclipse.ecl.core:Script">
    <content><![CDATA[get-view "Fantom Explorer" | get-tree | select "fantest/fan" | get-menu "New/Fantom Class" | click
with [get-window "New Fantom Class"] {
    get-editbox -after [get-label "Name:"] | set-text alfa
    get-button Finish | click
}
with [get-editor "alfa.fan" | get-editbox] {
    set-text-offset 1 2
    type-text "void test() {"
    key-type CR
    type-text "if(a."
    show-content-assist
}
get-window -from "CompletionProposalPopup.createProposalSelector()" | get-table | select RectangleBase | click -nowait 
    -default
with [get-editor "alfa.fan" | get-editbox] {
    type-text " == b."
    show-content-assist
}
get-window -from "CompletionProposalPopup.createProposalSelector()" | get-table | select RectangleBase | click -nowait 
    -default
get-editor "alfa.fan" | get-editbox | get-property value 
    | equals "class alfa{void test() {  if(a.RectangleBase == b.RectangleBase)}}" | verify-true]]></content>
  </content>
