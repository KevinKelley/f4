  <content xsi:type="org.eclipse.ecl.core:Script">
    <content><![CDATA[get-button "Open Perspective" | click
get-menu "Other..." | click
get-window "Open Perspective" | get-table | get-item "Fantom \\(default\\)" | get-property caption 
    | equals "Fantom (default)" | verify-true
with [get-window "Open Perspective"] {
    get-table | select "Fantom \\(default\\)"
    get-button OK | click
}
with [get-tab-folder -index 2] {
    get-property pages -index 0 | equals Problems | verify-true
    get-property pages -index 1 | equals Tasks | verify-true
    get-property pages -index 2 | equals Console | verify-true
    get-property pages -index 3 | equals "Error Log" | verify-true
}
get-tab-folder -index 3 | get-property pages -index 0 | equals Outline | verify-true]]></content>
  </content>
